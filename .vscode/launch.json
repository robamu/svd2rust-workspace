    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug Non-Working",
            "preLaunchTask": "rust: cargo build svd2rust",
            "program": "${workspaceRoot}/target/debug/svd2rust",
            "args": ["-i", "${workspaceRoot}/va108xx-rs/va108xx.svd", "--log", "trace"],
            "cwd": "${workspaceRoot}/va108xx-rs",
            "sourceLanguages": ["rust"]
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug Working",
            "preLaunchTask": "rust: cargo build svd2rust",
            "program": "${workspaceRoot}/target/debug/svd2rust",
            "args": ["-i", "${workspaceRoot}/va108xx-rs/working_va108xx.svd", "--log", "trace"],
            "cwd": "${workspaceRoot}/va108xx-rs",
            "sourceLanguages": ["rust"]
        }
    ],
}
